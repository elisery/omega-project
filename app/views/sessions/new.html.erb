<div class="container-fluid col-md-4 col-md-offset-1 jumbotron">
  <div class="container">
    <h3> Sign In</h3>
  </div>
  <div class="container">
    <%= form_for :session, url: session_path do |f| %>
        <div class="form-group">
          <%= f.label :email %> <br>
          <%= f.email_field :email %>
        </div>
      
        <div class="form-group">
          <%= f.label :password %> <br>
          <%= f.password_field :password %>
        </div>
        <%= f.submit "Sign In", class: "btn-sm btn-dark" %>
    <% end %>
  </div>
</div>
